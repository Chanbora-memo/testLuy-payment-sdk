{
  "name": "testluy-payment-sdk",
  "version": "3.6.1",
  "description": "SDK for TestLuy Payment Simulator with Cloudflare resilience",
  "main": "index-enhanced.js",
  "type": "module",
  "exports": {
    ".": "./index-enhanced.js",
    "./legacy": "./index.js"
  },
  "files": [
    "index-enhanced.js",
    "index.js",
    "config.js",
    "validation.js",
    "http/**/*.js",
    "README.md",
    "README-ENHANCED.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node test-enhanced.js",
    "test:debug": "node test-debugging-monitoring.js",
    "test:browser": "node test-browser-compatibility.js",
    "test:logger": "node test-logger.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --testPathPattern=tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --testPathPattern=tests/integration",
    "test:all": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js"
  },
  "keywords": [
    "payment",
    "simulator",
    "sdk",
    "cloudflare",
    "resilience",
    "api"
  ],
  "author": "Chanbora Seng",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chanboraseng/testluy-payment-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chanboraseng/testluy-payment-sdk/issues"
  },
  "homepage": "https://github.com/chanboraseng/testluy-payment-sdk#readme",
  "dependencies": {
    "axios": "^1.7.3",
    "dotenv": "^16.4.7",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "undici": "^7.11.0"
  }
}
